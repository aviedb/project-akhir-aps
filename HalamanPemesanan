class HalamanPemesanan
  Declarations
    PesanController controller

  void setController (PesanController controller)
  begin
    this.controller = controller
  end

  void showFilms (Films[] films)
  begin
    print all films
  end

  void showBioskops (Bioskop[] bioskops)
  begin
    print all bioskops
  end

  void tampilkanDashboardScreen ()
  begin
    route to (new Dashboard())
  end

  void tampilkanPesananError (error)
  begin
    show error message
  end
