class HalamanPemilihanFilm
  Declarations
    FilmController controller
    film_id

  void setController(FilmController controller)
  begin
    this.controller = controller
  end

  void tampilkanPesanError()
  begin
    menampilkan pesan error
  end

  void memilihFilm()
  begin
    this.controller.memilihFilm(this.film_id)
  end
